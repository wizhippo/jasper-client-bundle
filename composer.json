{
  "name": "wizhippo/jasper-client-bundle",
  "type": "symfony-bundle",
  "keywords": [
    "jasper",
    "client"
  ],
  "description": "JasperServer Client and utils.",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Douglas Hammond",
      "email": "wizhippo@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "symfony/config": "^2.3 || ^3.0",
    "symfony/dependency-injection": "^2.3 || ^3.0",
    "symfony/http-kernel": "^2.3 || ^3.0",
    "jaspersoft/rest-client": "^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Wizhippo\\Bundle\\JasperClientBundle\\": "bundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wizhippo\\Bundle\\JasperClientBundle\\Tests\\": "tests/"
    }
  }
}
